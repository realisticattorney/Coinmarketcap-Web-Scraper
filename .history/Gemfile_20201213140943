require 'HTTParty'
require 'Nokogiri'

def Scraper
   url = "https:/blockwork.cc/" # URL we want to target
   unparsed_page = HTTParty.get(url) # get request to that URL whick gives back the raw HTML
   parsed_page = Nokogiri::HTML(unparsed_page)                # we bring nokogiri to parse that page
end